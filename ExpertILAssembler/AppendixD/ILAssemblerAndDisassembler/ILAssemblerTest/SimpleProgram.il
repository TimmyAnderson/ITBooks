.assembly extern mscorlib { }
.assembly SimpleProgram { }
.module SimpleProgram.exe
//-------------------------------------------------------------------------------------------------------
.class public Program
{
//-------------------------------------------------------------------------------------------------------
	.method static public void SimpleTest()
	{
		ldstr "Hello WORLD !"
		call void [mscorlib]System.Console::WriteLine(string)
		ret
	}
//-------------------------------------------------------------------------------------------------------
}
//-------------------------------------------------------------------------------------------------------
//-------------------------------------------------------------------------------------------------------
//-------------------------------------------------------------------------------------------------------
.method public static void Exec()
{
	.entrypoint

	call void Program::SimpleTest()

	ret
}
//-------------------------------------------------------------------------------------------------------