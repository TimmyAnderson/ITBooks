{
	"rules":
	[
		{
			"name": "deleteTempAfter1Day",
			"enabled": true,
			"type": "Lifecycle",
			"definition":
			{
				"filters":
				{
					"blobTypes": ["blockBlob"],
					"prefixMatch": ["myblobcontainer/MyFile.tmp"]
				},
				"actions":
				{
					"baseBlob":
					{
						"delete":
						{
							"daysAfterModificationGreaterThan": 1
						}
					}
				}
			}
		}
	]
}