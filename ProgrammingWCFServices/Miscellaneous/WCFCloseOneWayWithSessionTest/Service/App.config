<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
      <service name="WCFCloseOneWayWithSessionTest.CTestService"
               behaviorConfiguration="ServiceBehavior">
        <endpoint address="net.tcp://localhost:9000"
                  binding="netTcpBinding"
                  contract="WCFCloseOneWayWithSessionTest.ITestService"
                  bindingConfiguration="EndpointBinding"
                  behaviorConfiguration="EndpointBehavior">
        </endpoint>
      </service>
    </services>

    <bindings>
      <netTcpBinding>
        <binding name="EndpointBinding">
          <security mode="Message">
          </security>
        </binding>
      </netTcpBinding>
    </bindings>

    <behaviors>
      <serviceBehaviors>
        <behavior name="ServiceBehavior">
          <serviceCredentials>
            <serviceCertificate findValue="ZEUS2"
                                storeLocation="LocalMachine"
                                storeName="TrustedPeople"
                                x509FindType="FindBySubjectName"/>
          </serviceCredentials>
        </behavior>
      </serviceBehaviors>

      <endpointBehaviors>
        <behavior name="EndpointBehavior">
        </behavior>
      </endpointBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>