@model CSimpleChild[]

@{
	Layout=null;
}

<!DOCTYPE html>
<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<link rel="stylesheet" href="~/StaticFiles/Styles/Styles.css" />
	<link rel="stylesheet" href="~/StaticFiles/ClientLibraries/Bootstrap/dist/css/bootstrap.css" />
	<title>View Children</title>
</head>
<body>
	<div class="OUTER_BOX">
		<table class="table table-bordered table-striped">
			<tr>
				<th>First name</th>
				<th>Last name</th>
				<th>Age</th>
				<th>Sex</th>
			</tr>

			@foreach(CSimpleChild SimpleChild in @Model)
			{
				<tr>
					<td>
						[@SimpleChild.FirstName]
					</td>
						
					<td>
						[@SimpleChild.LastName]
					</td>
						
					<td>
						[@SimpleChild.Age]
					</td>
						
					<td>
						[@SimpleChild.Sex] !
					</td>
				</tr>
			}
		</table>
	</div>
</body>
</html>