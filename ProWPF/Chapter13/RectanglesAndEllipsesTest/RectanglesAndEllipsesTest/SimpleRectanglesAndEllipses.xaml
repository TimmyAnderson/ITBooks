<Window x:Class="RectanglesAndEllipsesTest.SimpleRectanglesAndEllipses"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="RectanglesAndEllipsesTest" Height="300" Width="300">
  <StackPanel>
    <!-- Musim zadefinovat Stroke a Fill ABY BOLI Rectangle a Ellipse viditelne. -->
    <Ellipse Fill="Yellow" Stroke="Blue" Height="50" Width="100" Margin="5" HorizontalAlignment="Left"/>
    <Rectangle Fill="Yellow" Stroke="Blue" Height="50" Width="100" Margin="5" HorizontalAlignment="Left"/>
  </StackPanel>
</Window>