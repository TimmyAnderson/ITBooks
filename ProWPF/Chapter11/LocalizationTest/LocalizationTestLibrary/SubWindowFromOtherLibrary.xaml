<Window x:Uid="Window_1" x:Class="LocalizationTestLibrary.SubWindowFromOtherLibrary"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:s="clr-namespace:System;assembly=mscorlib"
    Title="LocalizationTestLibrary" Height="300" Width="300" Name="MWindow">
  <Window.Resources>
    <s:String x:Uid="s:String_1" x:Key="MB_Text">Do You want to Close Window ?</s:String>
    <s:String x:Uid="s:String_2" x:Key="MB_Caption">Question</s:String>
  </Window.Resources>
  <StackPanel x:Uid="StackPanel_1">
    <Label x:Uid="LOC_Label1" Content="Hi Timmy !"/>
    <Label x:Uid="LOC_Label2" Height="50" Width="50">
      <Image x:Uid="LOC_Image" Source="pack://application:,,,/Images/HappyFace.jpg"/>
    </Label>
    <Button x:Uid="LOC_Button" Click="OnButtonClick" Content="Close"/>
  </StackPanel>
</Window>