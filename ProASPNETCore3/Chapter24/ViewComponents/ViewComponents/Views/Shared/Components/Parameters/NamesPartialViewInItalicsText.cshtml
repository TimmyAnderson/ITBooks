@model CName[]

<!-- !!!!! PARTIAL VIEW pre VIEW COMPONENT 'Parameters'. -->

<div class="OUTER_BOX">
	<table class="table table-bordered table-striped">
		<tr>
			<th>ID</th>
			<th>First name</th>
			<th>Last name</th>
			<th>Age</th>
		</tr>

		@foreach(CName Name in @Model)
		{
			<tr>
				<th>
					@Name.ID
				</th>
						
				<td>
					[<i>@Name.FirstName</i>]
				</td>
						
				<td>
					[<i>@Name.LastName</i>]
				</td>
						
				<td>
					[<i>@Name.Age</i>] !
				</td>
			</tr>
		}
	</table>
</div>