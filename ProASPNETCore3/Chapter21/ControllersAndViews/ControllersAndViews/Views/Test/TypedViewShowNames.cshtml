<!-- !!!!! VIEW je TYPED, pretoze ma definovany MODEL OBJECT TYPE. -->
@model CName[]

@{
	Layout=null;
}

<!DOCTYPE html>
<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<link rel="stylesheet" href="~/StaticFiles/Styles/Styles.css" />
	<link rel="stylesheet" href="~/StaticFiles/ClientLibraries/Bootstrap/css/bootstrap.css" />
	<title>Show Names</title>
</head>
<body>
	<div class="OUTER_BOX">
		<h5>VIEW SOURCE [Test/TypedViewShowNames.cshtml] !</h5>

		<table class="table table-bordered table-striped">
			<tr>
				<th>
					ID
				</th>

				<th>
					First name
				</th>

				<th>
					Last name
				</th>

				<th>
					Age
				</th>
			</tr>

			@{
				// !!! MODEL je UNTYPED, takze je EVALUATED uz v COMPILE-TIME.
				foreach(CName Name in Model)
				{
					<tr>
						<td>
							[@Name.ID]
						</td>

						<td>
							[@Name.FirstName]
						</td>

						<td>
							[@Name.LastName]
						</td>

						<td>
							[@Name.Age] !
						</td>
					</tr>
				}
			}
		</table>
	</div>
</body>
</html>